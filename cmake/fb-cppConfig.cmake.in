@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Boost.DLL and Boost.Multiprecision are header-only - just need Boost headers
find_dependency(Boost)

# Firebird5 client library (transitive dependency)
find_dependency(Firebird5 CONFIG PATHS "$ENV{ASV_PLAT_PORTS}/firebird5" NO_DEFUALT_PATH) 

include("${CMAKE_CURRENT_LIST_DIR}/fb-cppTargets.cmake")

check_required_components(fb-cpp)
